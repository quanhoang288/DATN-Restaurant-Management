{
  "name": "final-project-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db-create": "npx sequelize-cli db:create --config src/database/config/config.js",
    "seed": "npx sequelize-cli db:seed:all --seeders-path src/database/seeders",
    "migration:create": "npx sequelize-cli migration:create --migrations-path src/database/migrations --name",
    "migrate": "npx sequelize-cli db:migrate --config src/database/config/config.js --migrations-path src/database/migrations",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config src/database/config/config.js --migrations-path src/database/migrations",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all --config src/database/config/config.js --migrations-path src/database/migrations",
    "db-init": "npm run db-create & npm run migrate",
    "sequelize": "npx sequelize-cli",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.2.2",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "sequelize": "^6.18.0",
    "sequelize-paginate": "^1.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3"
  }
}
